{
  "name": "inhousesoft-web",
  "version": "0.2.0",
  "private": true,
  "description": "Inhousesoft Landing page",
  "author": "Inhousesoft <inhousesoft.official@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Inhousesoft-lab/inhousesoft-web.git"
  },
  "scripts": {
    "clean": "rimraf dist/{css/*,js/*,images/*}",
    "autoprefixer": "postcss -u autoprefixer -r dist/css/*",
    "minify": "css-minify -d src/assets/css -o dist/css",
    "imagemin": "imagemin src/assets/images/* -o dist/images",
    "build:css": "run-s minify autoprefixer",
    "build:images": "run-s imagemin",
    "build": "run-s build:*"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "browser-sync": "^3.0.2",
    "css-minify": "^2.0.0",
    "imagemin-cli": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.0",
    "rimraf": "^5.0.5"
  }
}
